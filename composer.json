{
    "name": "innmind/rest-bundle",
    "type": "symfony-bundle",
    "description": "This Bundle wraps the innmind rest server library",
    "keywords": ["rest", "api"],
    "homepage": "http://github.com/Innmind/RestBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/Innmind/RestBundle/issues"
    },
    "require": {
        "php": ">=5.5",
        "innmind/rest-server": "~2",
        "symfony/http-kernel": "~2.7|~3.0",
        "symfony/dependency-injection": "~2.7|~3.0",
        "symfony/config": "~2.7|~3.0",
        "symfony/framework-bundle": "~2.7|~3.0",
        "innmind/rest-client": "~2",
        "symfony/filesystem": "~2.7|~3.0",
        "innmind/url-resolver": "^1.0",
        "guzzlehttp/guzzle": "~5.0",
        "symfony/console": "~2.7|~3.0"
    },
    "autoload": {
        "psr-0": {
            "Innmind\\RestBundle": ""
        }
    },
    "target-dir": "Innmind/RestBundle",
    "require-dev": {
        "innmind/neo4j-onm": "~1.0"
    }
}
